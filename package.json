{
  "name": "notification-system",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "start:service": "nest start",
    "start:worker:email": "WORKER_TYPE=email node dist/src/processors/email.processor",
    "start:worker:sms": "WORKER_TYPE=sms node dist/src/processors/sms.processor",
    "start:worker:fcm": "WORKER_TYPE=fcm node dist/src/processors/fcm.processor",
    "start:worker:whatsapp": "WORKER_TYPE=whatsapp node dist/src/processors/whatsapp.processor",
    "start:all": "concurrently \"npm:start:service\" \"npm:start:worker:*\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:notifications": "jest --testPathPattern=notifications",
    "test:templates": "jest --testPathPattern=templates",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f",
    "docker:ps": "docker compose ps",
    "docker:build": "docker build -t notification-service:latest -f docker/Dockerfile .",
    "docker:build:worker": "docker build -t notification-worker:latest -f docker/Dockerfile.worker .",
    "docker:local:up": "docker-compose -f docker/docker-compose.local.yml up -d",
    "docker:local:down": "docker-compose -f docker/docker-compose.local.yml down",
    "docker:local:logs": "docker-compose -f docker/docker-compose.local.yml logs -f",
    "proto:generate": "sh scripts/generate-proto.sh",
    "proto:install": "npm install --save-dev ts-proto @grpc/grpc-js @grpc/proto-loader",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:reset": "ts-node src/database/seeds/reset-database.ts",
    "seed": "ts-node src/database/seeds/run-all-seeds.ts",
    "seed:keycloak": "ts-node src/database/seeds/keycloak/seed-keycloak-users.ts",
    "seed:database": "ts-node src/database/seeds/run-all-seeds.ts",
    "seed:providers": "ts-node src/database/seeds/run-all-seeds.ts",
    "seed:users": "ts-node src/database/seeds/keycloak/seed-keycloak-users.ts",
    "seed:all": "npm run seed:database && npm run seed:keycloak"
  },
  "dependencies": {
    "@apollo/subgraph": "^2.9.3",
    "@grpc/grpc-js": "^1.12.4",
    "@grpc/proto-loader": "^0.7.15",
    "@keycloak/keycloak-admin-client": "^26.5.0",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/microservices": "^11.1.11",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/platform-ws": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.11",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.53.0",
    "@opentelemetry/exporter-jaeger": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.56.0",
    "@sendgrid/mail": "^8.1.6",
    "@whiskeysockets/baileys": "^7.0.0-rc.9",
    "@wppconnect-team/wppconnect": "^1.37.9",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.13.2",
    "bullmq": "^5.66.4",
    "cache-manager": "^7.2.7",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "compression": "^1.8.1",
    "concurrently": "^9.1.2",
    "csv-parser": "^3.2.0",
    "drizzle-orm": "^0.45.1",
    "firebase-admin": "^13.6.0",
    "graphql": "^16.10.0",
    "graphql-subscriptions": "^2.0.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.0",
    "jsonwebtoken": "^9.0.3",
    "jwks-rsa": "^3.2.0",
    "kafkajs": "^2.2.4",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "papaparse": "^5.5.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "postgres": "^3.4.8",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "twilio": "^5.11.2",
    "uuid": "^13.0.0",
    "winston": "^3.19.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.19.3",
    "@types/nodemailer": "^7.0.4",
    "@types/papaparse": "^5.5.2",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
